{
  "name": "Uber",
  "terms": {
    "Terms of Service": {
      "fetch": "https://www.uber.com/legal/fr/document/?name=general-terms-of-use&country=france&lang=fr",
      "select": [
        "#legal-info"
      ],
      "executeClientScripts": true
    },
    "Privacy Policy": {
      "fetch": "https://www.uber.com/legal/fr/document/?name=privacy-notice&country=france&lang=fr",
      "select": [
        "#legal-info"
      ],
      "remove": [
        "h2 > img"
      ],
      "executeClientScripts": true
    },
    "Trackers Policy": {
      "fetch": "https://www.uber.com/legal/fr/document/?name=cookie-notice&country=france&lang=fr",
      "executeClientScripts": true,
      "select": [
        "#legal-info"
      ]
    }
  }
}
