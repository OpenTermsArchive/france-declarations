{
  "name": "TikTok",
  "documents": {
    "Terms of Service": {
      "fetch": "https://www.tiktok.com/legal/terms-of-service-eea?lang=fr",
      "select": [
        ".article-wrapper"
      ],
      "remove": [
        ".language-selection",
        ".dropdown-title > img"
      ]
    },
    "Trackers Policy": {
      "fetch": "https://www.tiktok.com/legal/tiktok-website-cookies-policy?lang=fr",
      "select": [
        ".article-wrapper"
      ],
      "remove": [
        "#settings-button"
      ]
    },
    "Copyright Claims Policy": {
      "fetch": "https://www.tiktok.com/legal/copyright-policy?lang=fr",
      "select": [
        ".article-wrapper"
      ]
    },
    "Privacy Policy": {
      "fetch": "https://www.tiktok.com/legal/privacy-policy-eea?lang=fr",
      "select": [
        "[data-testid=\"policy-card\"]"
      ],
      "executeClientScripts": true
    }
  }
}
