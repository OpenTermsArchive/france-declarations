{
  "name": "Galeries Lafayette",
  "documents": {
    "Commercial Terms": {
      "fetch": "https://www.galerieslafayette.com/service/conditions-generals",
      "select": [
        ".mainContent"
      ],
      "remove": ".faqQuestion, .topLink, [href=\"#top\"]",
      "filter": ["addArticles", "deleteReturnToTheTop", "addTableHeaderOnCommercialTerms", "commercialTermsTableH2toH3", "addExternalLinks"],
      "executeClientScripts": true
    },
    "Privacy Policy": {
      "fetch": "https://www.galerieslafayette.com/service/service-confidence",
      "select": [
        ".faqQuestion > ul > li",
        ".faqRound"
      ],
      "filter": ["addTableHeader", "mergeTables", "removeEmptyItemsInTOC", "setTitleH4"],
      "executeClientScripts": true
    }
  }
}
